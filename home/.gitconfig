[user]
	email = 10003601+a@users.noreply.github.com
	name = anyumekawa
[push]
	default = simple
[color]
	ui = auto
[core]
	editor = nvim
[log]
	decorate = short
[alias]
	add-commit = !git add -A && git commit -v
	addp = add -p
	alias = config --get-regexp ^alias\\.
	branchall = branch -a
	commitv = commit -v
	ignore = "!f () { for i in \"$@\" ; do echo \"$i\" >> .gitignore ; done ; } ; f"
	initcommit = !git init && git commit --allow-empty -m \"Inital commit.\"
	merge-noff = merge --no-ff
	pull-rebase = pull --rebase
	push-u = push -u origin HEAD
	refresh = fetch --prune
	reset-to-head = reset --hard HEAD
	staged = diff --cached
	tree = log --graph --pretty=format:'%C(auto)%h%d %Creset%s - %Cblue%cn, %Cgreen%cr%Creset'
	uncommit = reset --soft HEAD~
	unstage = reset HEAD
