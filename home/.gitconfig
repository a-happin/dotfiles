[user]
	email = 10003601+a@users.noreply.github.com
	name = anyumekawa
[push]
	default = simple
[color]
	ui = auto
[core]
	editor = nvim
[log]
	decorate = short
[alias]
	add-commit = !git add -A && git commit -v
	addp = add -p
	alias = config --get-regexp ^alias\\.
	amend = commit --amend
	commitv = commit -v
	delete-current-branch = !git detach && git branch -d @{-1} && :
	detach = checkout --detach
	hash = rev-parse HEAD
	ignore = "!f () { for i in \"$@\" ; do echo \"$i\" >> .gitignore ; done ; } ; f"
	initcommit = !git init && git commit --allow-empty -m \"Initial commit.\"
	merge-noff = merge --no-ff
	pull-rebase = pull --rebase
	push-u = push -u origin HEAD
	refresh = fetch --prune
	reset-to-head = reset --hard HEAD
	staged = diff --cached
	touch = commit --amend --no-edit --date=now
	tree = log --graph --all --pretty=format:'%C(auto)%h%d%C(reset) - %s %C(blue)by %an%C(reset), %C(green)%cr%C(reset)'
	uncommit = reset --soft HEAD~
	unstage = reset HEAD
	wip = commit -m \"WIP: \" --edit
